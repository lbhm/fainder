@inproceedings{kaoudi_cost-based_2017,
	location = {Chicago, {IL}},
	title = {A Cost-based Optimizer for Gradient Descent Optimization},
	isbn = {978-1-4503-4197-4},
	url = {http://dl.acm.org/citation.cfm?doid=3035918.3064042},
