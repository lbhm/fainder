@article{xie_query_2018,
	title = {Query Log Compression for Workload Analytics},
	volume = {12},
	issn = {2150-8097},
	url = {https://dl.acm.org/doi/10.14778/3291264.3291265},
	doi = {10.14778/3291264.3291265},
	abstract = {Analyzing database access logs is a key part of performance tuning, intrusion detection, benchmark development, and many other database administration tasks. Unfortunately, it is common for production databases to deal with millions or more queries each day, so these logs must be summarized before they can be used. Designing an appropriate summary encoding requires trading oÔ¨Ä between conciseness and information content. For example: simple workload sampling may miss rare, but high impact queries. In this paper, we present {LogR}, a lossy log compression scheme suitable for use in many automated log analytics tools, as well as for human inspection. We formalize and analyze the space/Ô¨Ådelity trade-oÔ¨Ä in the context of a broader family of ‚Äúpattern‚Äù and ‚Äúpattern mixture‚Äù log encodings to which {LogR} belongs. We show through a series of experiments that {LogR} compressed encodings can be created eÔ¨Éciently, come with provable information-theoretic bounds on their accuracy, and outperform state-of-art log summarization strategies.},
	pages = {183--196},
	number = {3},
	journaltitle = {Proceedings of the {VLDB} Endowment},
