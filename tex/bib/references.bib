@article{dodge_quality_2017,
	title = {Quality Resilient Deep Neural Networks},
	url = {http://arxiv.org/abs/1703.08119},
	abstract = {We study deep neural networks for classiÔ¨Åcation of images with quality distortions. We Ô¨Årst show that networks Ô¨Åne-tuned on distorted data greatly outperform the original networks when tested on distorted data. However, Ô¨Ånetuned networks perform poorly on quality distortions that they have not been trained for. We propose a mixture of experts ensemble method that is robust to different types of distortions. The ‚Äúexperts‚Äù in our model are trained on a particular type of distortion. The output of the model is a weighted sum of the expert models, where the weights are determined by a separate gating network. The gating network is trained to predict optimal weights for a particular distortion type and level. During testing, the network is blind to the distortion level and type, yet can still assign appropriate weights to the expert models. We additionally investigate weight sharing methods for the mixture model and show that improved performance can be achieved with a large reduction in the number of unique network parameters.},
	journaltitle = {{arXiv}:1703.08119},
	author = {Dodge, Samuel and Karam, Lina},
	urldate = {2021-04-27},
	date = {2017-03-23},
